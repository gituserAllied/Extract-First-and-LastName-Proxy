<ProxyEndpoint name="default">
  <HTTPProxyConnection>
    <BasePath>/printfilter</BasePath>
    <VirtualHost>default</VirtualHost>
  </HTTPProxyConnection>

  <PreFlow name="PreFlow">
    <Request/>
    <Response>
      <Step>
        <Name>ExtractNameFields</Name>
      </Step>
      <Step>
        <Name>AssignFilteredResponse</Name>
      </Step>
    </Response>
  </PreFlow>

  <Flows/>

  <PostFlow name="PostFlow">
    <Request/>
    <Response/>
  </PostFlow>

  <RouteRule name="default">
    <TargetEndpoint>default</TargetEndpoint>
  </RouteRule>
</ProxyEndpoint>
